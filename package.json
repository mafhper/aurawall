{
    "name": "aurawall-v0.1",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "concurrently \"npm run app\" \"npm run promo\"",
        "start": "npx vite preview --port 3000",
        "app": "vite",
        "promo": "vite --config website/vite.config.ts",
        "build": "npm run build-promo && npm run build-app",
        "build-app": "vite build",
        "build-promo": "vite build --config website/vite.config.ts",
        "preview": "vite preview",
        "health": "node scripts/health.cjs",
        "health:fast": "node scripts/health.cjs --quick",
        "audit": "node scripts/audit-runner.cjs",
        "audit:all": "npm run audit && npm run health",
        "audit:promo": "node scripts/audit-runner.cjs --filter=promo",
        "audit:promo:mobile": "cross-env AUDIT_MOBILE=true node scripts/audit-landing.cjs",
        "audit:promo:prod": "cross-env AUDIT_URL=https://mafhper.github.io/aurawall node scripts/audit-landing.cjs",
        "audit:app": "node scripts/audit-runner.cjs --filter=app",
        "optimize:images": "node scripts/optimize-images.cjs",
        "icons": "node scripts/distribute-icons.cjs",
        "generate:bgs": "node scripts/generate-bgs.cjs",
        "logs": "node scripts/fetch-changelog.js",
        "test:structure": "node scripts/test-structure.cjs",
        "test:security": "npm audit",
        "test:i18n": "node scripts/test-i18n.cjs",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "test:lint": "node scripts/test-lint.cjs",
        "test:perf": "node scripts/test-perf.cjs"
    },
    "dependencies": {
        "i18next": "^25.7.2",
        "i18next-browser-languagedetector": "^8.2.0",
        "lucide-react": "^0.556.0",
        "lz-string": "^1.5.0",
        "react": "^19.2.1",
        "react-dom": "^19.2.1",
        "react-i18next": "^16.4.0",
        "react-router-dom": "^7.10.1"
    },
    "devDependencies": {
        "@eslint/js": "^9.39.2",
        "@tailwindcss/postcss": "^4.1.17",
        "@types/node": "^22.14.0",
        "@vitejs/plugin-react": "^5.0.0",
        "autoprefixer": "^10.4.22",
        "chrome-launcher": "^1.2.1",
        "concurrently": "^9.2.1",
        "cross-env": "^10.1.0",
        "eslint": "^9.39.2",
        "eslint-plugin-react-hooks": "^7.0.1",
        "eslint-plugin-react-refresh": "^0.4.24",
        "globals": "^16.5.0",
        "lighthouse": "^13.0.1",
        "postcss": "^8.5.6",
        "sharp": "^0.34.5",
        "tailwindcss": "^4.1.17",
        "typescript": "~5.8.2",
        "typescript-eslint": "^8.49.0",
        "vite": "^6.2.0"
    }
}